{
    "name" : "sammousa/limesurvey",
    "description" : "Limesurvey for composer.",
    "license" : "GPL-2.0",
    "minimum-stability": "dev",
    "config": {
        "vendor-dir": "application/vendor"
    },
    "require": {
		"2amigos/yiiwheels": "2.*",
		"yiisoft/yii" : "1.*",
		"tecnick.com/tcpdf": "dev-master",
		"phpmailer/phpmailer": "dev-master",
		"jsonrpc/jsonrpc" : "dev-master",
        "ar-php/ar-php": "dev-master",
        "guzzlehttp/guzzle": "~5.0@dev",
        "cakephp/utility": "3.*",
        "sam-it/php-auto-update": "dev-master",
        "icecave/semver": "dev-master",
        "hafriedlander/php-peg": "dev-master",
        "sam-it/yii1-translate": "dev-master",
        "sam-it/forms": "dev-master",
        "mk-j/php_xlsxwriter": "0.*",

        "zendframework/zend-diactoros": "^1.0@dev",
        "bower-asset/ajaxq": "^0.0.3@dev",
        "bower-asset/jquery": "2.*",
        "bower-asset/jquery.actual": "1.*",
        "bower-asset/papaparse": "4.1",
        "bower-asset/tinymce": "~4.1@dev",
        "bower-asset/remarkable-bootstrap-notify": "3.*",
        "bower-asset/select2-bootstrap-css": "1.*",
        "bower-asset/jquery-ui": "1.*",
        "bower-asset/jqueryui-timepicker-addon": "1.5.*",
        "studio-42/elfinder": "2.1.x-dev"
    },
	"require-dev" : {
		
	},
    "autoload" : {
        "classmap" : [
            "application/extensions/SettingsWidget",
            "application/core/db",
            "application/core",
            "application/vendor/2amigos/yiiwheels",
            "application/vendor/yiisoft/yii/framework/cli/commands",
            "application/components",
            "application/helpers"
        ],
        "psr-4" : {
            "ls\\" : [
                "application"
            ],
            "ls\\cli\\" : [
                "application/commands"
            ],
            "ls\\expressionmanager\\" : [
                "application/libraries/ExpressionManager"
            ],
            "ls\\pluginmanager\\" : [
                "application/libraries/PluginManager",
                "application/libraries/PluginManager/Storage"
            ],
            "ls\\import\\" : [
              "application/helpers/import"
            ]
        }
    },
    "extra" : {
        "asset-installer-paths": {
            "bower-asset-library" : "components",
            "npm-asset-library" : "components"
        }
    }

}
