{
    "name" : "sammousa/limesurvey",
    "description" : "Limesurvey for composer.",
    "license" : "GPL-2.0",
    "minimum-stability": "dev",
    "config": {
        "vendor-dir": "protected/vendor"
    },
    "require": {
		"2amigos/yiiwheels": "2.*",
		"yiisoft/yii" : "1.*",
		"tecnickcom/tcpdf": "6.*",
		"phpmailer/phpmailer": "5.*",
		"jsonrpc/jsonrpc" : "1.*",
        "ar-php/ar-php": "dev-master",
        "guzzlehttp/guzzle": "~5.0@dev",
        "cakephp/utility": "3.*",
        "sam-it/php-auto-update": "dev-master",
        "icecave/semver": "3.*",
        "sam-it/yii1-translate": "dev-master",
        "sam-it/forms": "1.*",
        "mk-j/php_xlsxwriter": "0.*",
        "sam-it/yii1-bootstrap-treeview": "1.*",
        "studio-42/elfinder": "2.1.x-dev",
        "zendframework/zend-diactoros": "1.*",


        "bower-asset/ajaxq": "^0.0.3@dev",
        "bower-asset/jquery.actual": "1.*",
        "bower-asset/papaparse": "4.1",
        "bower-asset/tinymce": "~4.1@dev",
        "bower-asset/remarkable-bootstrap-notify": "3.*",
        "bower-asset/select2-bootstrap-css": "1.*",
        "bower-asset/jquery-ui": "1.*",
        "bower-asset/jqueryui-timepicker-addon": "1.5.*",
        "bower-asset/jsoneditor": "4.*",
        "bower-asset/webshim": "1.*",
        "bower-asset/TinyMCE-Placeholder": "^1.0@dev",
        "bower-asset/qtip2": "2.*",
        "bower-asset/jqueryui-touch-punch": "dev-master",
        "bower-asset/font-awesome": ">=4",
        "bower-asset/jqGrid" : "5.*",
        "bower-asset/js-cookie" : "2.*",
        "bower-asset/tablesorter" : "2.*",
        "bower-asset/leaflet": "0.7.3",
        "bower-asset/jquery-ace": "1.*",
        "bower-asset/jquery": "2.*"
    },
    "autoload" : {
        "classmap" : [
            "protected/extensions",
            "protected/extensions/SettingsWidget",
            "protected/core/db",
            "protected/core",
            "protected/vendor/2amigos/yiiwheels",
            "protected/vendor/crisu83/yiistrap/widgets",
            "protected/vendor/yiisoft/yii/framework/base",
            "protected/vendor/yiisoft/yii/framework/cli",
            "protected/vendor/yiisoft/yii/framework/web",
            "protected/vendor/yiisoft/yii/framework/utils",
            "protected/vendor/yiisoft/yii/framework/caching",
            "protected/vendor/yiisoft/yii/framework/collections",
            "protected/vendor/yiisoft/yii/framework/console",
            "protected/vendor/yiisoft/yii/framework/db",
            "protected/vendor/yiisoft/yii/framework/validators",
            "protected/vendor/yiisoft/yii/framework/i18n",
            "protected/helpers",
            "protected/models"
        ],
        "psr-4" : {
            "ls\\" : [
                "protected"
            ],
            "ls\\cli\\" : [
                "protected/commands"
            ],
            "ls\\expressionmanager\\" : [
                "protected/libraries/ExpressionManager"
            ],
            "ls\\pluginmanager\\" : [
                "protected/libraries/PluginManager",
                "protected/libraries/PluginManager/Storage"
            ],
            "ls\\import\\" : [
              "protected/helpers/import"
            ]
        }
    },
    "extra" : {
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    }
}
